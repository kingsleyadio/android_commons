<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BezelImageView">
        <attr name="maskDrawable"
            format="reference" />
        <attr name="borderDrawable"
            format="reference" />
        <attr name="desaturateOnPress"
            format="boolean" />
    </declare-styleable>

    <declare-styleable name="HeaderView">
        <attr name="headerTitle" format="string" />
        <attr name="headerColor" format="color" />
        <attr name="headerTextSize" format="dimension" />
        <attr name="dividerThickness" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Hint">
        <attr name="hint" format="string" />
    </declare-styleable>

</resources>
